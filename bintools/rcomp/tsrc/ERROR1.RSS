STRUCT TBUF
    {
    BUF buf; /* non-zero terminated text string */
    }

STRUCT LBUF
    {
    LTEXT txt; // leading-byte counted text string
    }

STRUCT LBUF16
    {
	LTEXT16 txt; // leading byte counted wide string
	}

STRUCT ARRAY
    {
    STRUCT items[];
    }

STRUCT BUTTON
    {
    WORD id;
	WORD flags;
    LTEXT txt;
	LTEXT txt2;
	LTEXT bmp;
    }

STRUCT FLPTED
