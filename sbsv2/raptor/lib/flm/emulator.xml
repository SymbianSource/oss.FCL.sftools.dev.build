		<param name='WIN32_HEADERS' default=''/>
		<param name='WIN32_LIBRARY' default=''/>
		<param name='WIN32_RESOURCE' default=''/>
	</interface>

	<interface name="Emulator.win32" extends="Emulator.mmp" abstract="true" flm="win32.flm">
		<param name='CC'/>
